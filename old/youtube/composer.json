{
    "name": "confrariaweb/youtube",
    "description": "Upload a video to YouTube with Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe ConfrariaWeb",
            "email": "hi@joeconfrariaweb.me"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.4|^6.0|^7.0|^8.0",
        "google/apiclient": "^2.7"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2",
        "phpunit/phpunit": "~5.0",
        "phpunit/phpcov": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ConfrariaWeb\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConfrariaWeb\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ConfrariaWeb\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "ConfrariaWeb\\Youtube\\Facades\\Youtube"
            }
        }
    }
}
